{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Enigma (GDB)",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/enigma.exe",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
        "miDebuggerPath": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for STL containers",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]

    
  }

  
  